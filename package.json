{
  "name": "notion-sdk-js",
  "version": "1.0.0",
  "description": "Forked Notion API client with customized scripts for generating aliases.",
  "engines": {
    "node": ">=12"
  },
  "main": "app.js",
  "homepage": "https://developers.notion.com/docs/getting-started",
  "bugs": {
    "url": "https://github.com/flyguy221-ERAU/notion-sdk-js/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/flyguy221-ERAU/notion-sdk-js/"
  },
  "keywords": [
    "notion",
    "notionapi",
    "rest",
    "notion-api",
    "database-aliases"
  ],
  "scripts": {
    "prepare": "npm run build",
    "prepublishOnly": "npm run checkLoggedIn && npm run lint && npm run test",
    "build": "tsc",
    "prettier": "prettier --write .",
    "lint": "prettier --check . && eslint . --ext .ts && cspell '**/*'",
    "test": "jest ./test",
    "check-links": "git ls-files | grep md$ | xargs -n 1 markdown-link-check",
    "prebuild": "npm run clean",
    "clean": "rm -rf ./build",
    "checkLoggedIn": "./scripts/verifyLoggedIn.sh",
    "start": "node ./scripts/get-props.js",
    "generate-aliases": "node alias-gen.js"
  },
  "author": "Jeremy Feagan",
  "license": "MIT",
  "files": [
    "build/package.json",
    "build/src/**"
  ],
  "dependencies": {
    "@notionhq/client": "^2.2.15",
    "@types/node-fetch": "^2.5.10",
    "axios": "^1.7.7",
    "csv-parser": "^3.0.0",
    "cytoscape": "^3.30.3",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "node-fetch": "^2.6.1",
    "fs": "^0.0.1-security",
    "tsc": "^2.0.4"
  },
  "devDependencies": {
    "@types/jest": "^28.1.4",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "cspell": "^5.4.1",
    "eslint": "^7.24.0",
    "jest": "^28.1.2",
    "markdown-link-check": "^3.8.7",
    "prettier": "^2.8.8",
    "ts-jest": "^28.0.5",
    "typescript": "^4.9.5"
  }
}
